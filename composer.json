{
    "name": "shornuk/craft-plausible",
    "description": "Plausible is a Craft CMS plugin that gives you handy dashboard widgets to display analytics data from Plausible.",
    "type": "craft-plugin",
    "version": "3.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "plausible"
    ],
    "support": {
        "docs": "https://github.com/shornuk/craft-plausible/blob/master/README.md",
        "issues": "https://github.com/shornuk/craft-plausible/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shorn",
            "homepage": "https://shorn.co.uk"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
          "shornuk\\plausible\\": "src/"
        }
    },
    "extra": {
        "name": "Plausible",
        "handle": "plausible",
        "developer": "Shorn",
        "developerUrl": "https://shorn.co.uk",
        "documentationUrl": "https://github.com/shornuk/craft-plausible/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/shornuk/craft-plausible/master/CHANGELOG.md",
        "components": {
            "plausible": "shornuk\\plausible\\services\\Plausible"
        },
        "class": "shornuk\\plausible\\Plausible"
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main"
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
